{
  "name": "alexicons",
  "version": "0.0.1",
  "description": "Use fun alexicons",
  "license": "MIT",
  "repository": "mralexgray/alexicons",
  "author": {
    "name": "Alex Gray",
    "email": "alex@mrgray.com",
    "url": "mrgray.com"
  },
  "bin": "src/cli.js",
  "scripts": {
	  "clean": "rm -r src/*.js",
		"build" : "coffee -c src/*.coffee",
	  "build:watch": "watch 'npm install -g' ./**/*",
		"test": "node src/test.js",
		"fixbin" : "sed -i '' '1s:^:#!/usr/bin/env node\\\n\\\n:' src/cli.js",
		"pretest" : "npm run build",
	  "prepublish": "npm run build && npm run fixbin"
  },
  "files": [ "src", "resources" ],
  "keywords": [
    "cli-app",
    "cli",
    "bin",
    "word",
    "words",
    "list",
    "array",
    "random",
    "rand",
    "animal",
    "cat",
    "cats",
    "kitten",
    "name",
    "names",
    "female",
    "male"
  ],
  "dependencies": {
    "meow": "^3.4.0",
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "ava": "0.0.4",
		"coffee-script" : "*"
  }
}
